{
    "name": "etelford/rachio",
    "description": "A PHP client for the Rachio API (http://rachio.com)",
    "authors": [
        {
            "name": "Erik Telford",
            "email": "e@eriktelford.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "ETelford\\Rachio\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ETelford\\Rachio\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4",
        "illuminate/support": "^5.2",
        "symfony/var-dumper": "^3.1"
    },
    "minimum-stability": "stable"
}
